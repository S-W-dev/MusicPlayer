{
  "manifest_version": 2,
  "name": "Music Player",
  "description": "Play Music",
  "version": "2.1.1",
  "browser_action": {
    "default_icon": "InvisiIcon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js_lib/jquery.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
