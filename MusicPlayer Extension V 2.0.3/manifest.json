{
	"manifest_version":2,

	"name":"Music Player",
	"description":"Play Music",
	"version":"2.0.3",

	"browser_action":{
		"default_icon":"InvisiIcon.png",
		"default_popup":"popup.html"
	},

	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},

	"permissions": [
		"activeTab"
	]
}
